{
  "kind": "collectionType",
  "collectionName": "numering_ranges",
  "info": {
    "singularName": "numering-range",
    "pluralName": "numering-ranges",
    "displayName": "numering_range"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "factus_id": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "prefijo": {
      "type": "string",
      "maxLength": 10,
      "required": true
    },
    "resolucion_dian": {
      "type": "string",
      "required": true
    },
    "desde": {
      "type": "integer",
      "required": true
    },
    "hasta": {
      "type": "integer",
      "required": true
    },
    "consecutivo_actual": {
      "type": "integer",
      "required": true,
      "default": 2
    },
    "tipo_documento": {
      "type": "enumeration",
      "default": "factura",
      "enum": [
        "factura",
        "nota_credito",
        "nota_debito",
        "factura_exportacion"
      ]
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "fecha_resolucion": {
      "type": "date"
    },
    "fecha_vencimiento": {
      "type": "date"
    },
    "observaciones": {
      "type": "text"
    }
  }
}
